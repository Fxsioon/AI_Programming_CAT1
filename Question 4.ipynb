   "source": [
    "from math import pi\n",
    "\n",
    "def area_of_circle(radius):\n",
    "    if radius <= 0:\n",
    "        return \"Invalid Input - Radius must be greater than 0.\"\n",
    "    area = pi * radius ** 2\n",
    "    return round(area, 2)\n",
    "\n",
    "r = 19\n",
    "print(f\"The area of a circle with radius {r} is: {area_of_circle(r)}\")"
   ]
